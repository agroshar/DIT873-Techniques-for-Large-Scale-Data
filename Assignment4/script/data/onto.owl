<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://www.semanticweb.org/julsz/ontologies/2020/4/assignment4"
         xmlns="http://www.semanticweb.org/julsz/ontologies/2020/4/assignment4#">

<owl:Ontology rdf:about="http://www.semanticweb.org/julsz/ontologies/2020/4/assignment4"/>

<owl:ObjectProperty rdf:about="#alumnusOf">
  <rdfs:domain rdf:resource="#Person"/>
  <rdfs:range rdf:resource="#University"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#bornIn">
  <rdfs:domain rdf:resource="#Person"/>
  <rdfs:range rdf:resource="#Place"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#employeeOf">
  <rdfs:domain rdf:resource="#Person"/>
  <rdfs:range rdf:resource="#Organisation"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#locatedIn">
  <rdfs:domain rdf:resource="#Organisation"/>
  <rdfs:range rdf:resource="#Place"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#countryName">
  <rdfs:domain rdf:resource="#Place"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#organisationName">
  <rdfs:domain rdf:resource="#Organisation"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#personName">
  <rdfs:domain rdf:resource="#Person"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#placeName">
  <rdfs:domain rdf:resource="#Place"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#yearFounded">
  <rdfs:domain rdf:resource="#University"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Person"/>

<owl:Class rdf:about="#University">
  <rdfs:subClassOf rdf:resource="#Organisation"/>
</owl:Class>

<owl:Class rdf:about="#Place"/>

<owl:Class rdf:about="#Organisation"/>

<Place rdf:about="#place1">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  <countryName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Germany</countryName>
  <placeName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Jena</placeName>
</Place>

<Place rdf:about="#place2">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  <countryName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Poland</countryName>
  <placeName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pozna≈Ñ</placeName>
</Place>

<Place rdf:about="#place3">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  <countryName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sweden</countryName>
  <placeName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lund Municipality</placeName>
</Place>

<Organisation rdf:about="#org1">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  <locatedIn rdf:resource="#place1"/>
  <organisationName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">University of Jena</organisationName>
</Organisation>

<University rdf:about="#uni1">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  <locatedIn rdf:resource="#place3"/>
  <organisationName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lund University</organisationName>
  <yearFounded rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1666</yearFounded>
</University>

<Person rdf:about="#person1">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  <alumnusOf rdf:resource="#uni1"/>
  <bornIn rdf:resource="#place2"/>
  <employeeOf rdf:resource="#org1"/>
  <personName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Aleksander Radler</personName>
</Person>


</rdf:RDF>
